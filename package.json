    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.12.18",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "mocha": "^7.0.1",
    "serverless-offline": "^5.12.1",
    "serverless-webpack": "^5.2.0",
    "sinon": "^8.1.1",
    "ts-loader": "^5.3.3",
    "typescript": "^3.2.4",
