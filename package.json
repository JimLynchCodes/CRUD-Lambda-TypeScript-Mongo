{
  "name": "crud-lambda-typescript-mongo",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "start": "npx serverless offline --skipCacheInvalidation --useSeparateProcesses start",
